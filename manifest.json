{
  "name": "USB Spinner Sample",
  "version": "0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["usb", "webview"],
  "optional_permissions": [ {"usbDevices": [{"vendorId": 10291, "productId": 1}]}]
}